    "svelte": "^3.44.3",
    "svelte-check": "^2.2.10",
    "svelte-preprocess": "^4.10.0",
    "ts-jest": "^27.1.1",
    "tslab": "^1.0.15",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "whatwg-fetch": "^3.6.2"
  },
  "type": "module",
  "dependencies": {
    "d3": "^7.2.1",
    "d3-sankey": "^0.12.3",
    "data-forge": "^1.9.2",
    "fp-ts": "^2.11.5",
