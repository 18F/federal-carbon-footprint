{
  "name": "web",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "link:ui": "ln -sf ./build _site",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "federalist": "npm run build && npm run link:ui && ls -al ./_site/",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "test": "jest",
    "test:integration": "jest --testPathPattern='.*\\.int\\.test\\.ts'",
    "test:unit": "jest --testPathPattern='^(?!.*\\.int\\.).*$'"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@sveltejs/adapter-static": "^1.0.0-next.29",
    "@sveltejs/kit": "1.0.0-next.307",
    "@types/cookie": "^0.4.1",
    "@types/d3": "^7.1.0",
    "@types/d3-sankey": "^0.11.2",
    "@types/jest": "^27.4.1",
    "@types/make-fetch-happen": "^9.0.2",
    "@types/memoizee": "^0.4.7",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "autoprefixer": "^10.4.4",
    "cssnano": "^5.1.7",
    "dotenv": "^16.0.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "jest": "^27.5.1",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "postcss-load-config": "^3.1.4",
    "postcss-url": "^10.1.3",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.6.0",
    "sass": "^1.49.11",
    "svelte": "^3.46.6",
    "svelte-check": "^2.4.6",
    "svelte-preprocess": "^4.10.5",
    "ts-jest": "^27.1.4",
    "tslab": "^1.0.15",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "whatwg-fetch": "^3.6.2"
  },
  "type": "module",
  "dependencies": {
    "d3": "^7.4.2",
    "d3-sankey": "^0.12.3",
    "data-forge": "^1.9.5",
    "fp-ts": "^2.11.9",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^2.0.1",
    "make-fetch-happen": "^10.1.2",
    "memoizee": "^0.4.15",
    "slugify": "^1.6.5",
    "uswds": "^2.13.0"
  }
}
