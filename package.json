    "@types/cookie": "^0.4.1",
    "@types/d3": "^7.1.0",
    "@types/d3-sankey": "^0.11.2",
    "@types/jest": "^27.0.3",
    "@types/memoizee": "^0.4.6",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.13",
    "dotenv": "^10.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "jest": "^27.4.5",
    "postcss": "^8.4.5",
